const githubNavPatch = () => {
  const base = window.location.hostname.includes("github.io") ? "/CTBAy" : "";

  const alreadyPatched = Array.from(
    document.querySelectorAll('a[href^="/"]')
  ).some((link) => link.href.includes(base));
  if (alreadyPatched) return;

  document.querySelectorAll('a[href^="/"]').forEach((link) => {
    link.href = base + link.getAttribute("href");
  });
};
githubNavPatch();
